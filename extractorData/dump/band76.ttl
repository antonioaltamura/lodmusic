dbr:Šarlo_Akrobata	rdf:type	dbo:Band ;
	foaf:name	"\u0160arlo Akrobata"@en ;
	dbo:associateBand	dbr:Suncokret ,
		<http://dbpedia.org/resource/Supernaut_(Serbian_band)> ,
		dbr:Katarina_II ,
