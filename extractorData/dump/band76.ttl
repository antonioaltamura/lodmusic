dbr:Dropkick_Murphys	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/DKMshamrock.jpg?width=300> ;
	foaf:name	"Dropkick Murphys"@en ;
	dbo:associateBand	<http://dbpedia.org/resource/Everybody_Out!> ,
