dbr:Commander_Cody_and_His_Lost_Planet_Airmen	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Commander_Cody_&_His_Lost_Planet_Airmen.jpg?width=300> ;
