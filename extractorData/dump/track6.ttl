	dbo:releaseDate	"2012-03-30"^^xsd:date ;
	foaf:name	"Let's Go"@en .
<http://dbpedia.org/resource/Let\u0027s_Go_(Pharoahe_Monch_song)>	rdf:type	dbo:MusicalWork ;
