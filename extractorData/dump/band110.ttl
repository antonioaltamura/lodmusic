dbr:KMFDM	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Kmfdm_live_in_2011.jpg?width=300> ;
	foaf:name	"KMFDM"@en ;
