dbr:Neutral_Milk_Hotel	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Neutral_Milk_Hotel_2014.jpg?width=300> ;
	foaf:name	"Neutral Milk Hotel"@en ;
	dbo:associateBand	dbr:The_Gerbils ,
		dbr:The_Olivia_Tremor_Control ,
		dbr:Major_Organ_and_the_Adding_Machine ,
