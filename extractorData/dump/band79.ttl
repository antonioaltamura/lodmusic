dbr:Cavalera_Conspiracy	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Cavalera_Conspiracy-Live-Norway_Rock_2010.jpg?width=300> ;
