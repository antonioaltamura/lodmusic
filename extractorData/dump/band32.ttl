dbr:Big_Brother_and_the_Holding_Company	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Janis_Joplin_Big_Brother_and_the_Holding_Company.jpg?width=300> ;
