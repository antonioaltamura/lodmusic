dbr:Blind_Guardian	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Blindguardian2015j.JPG?width=300> ;
	foaf:name	"Blind Guardian"@en ;
	dbo:associateBand	dbr:Schattentantz ,
		<http://dbpedia.org/resource/Demons_and_Wizards_(band)> ,
