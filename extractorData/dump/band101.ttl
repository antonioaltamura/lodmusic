dbr:Dave_Matthews_Band	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/DMBSummer2006.jpg?width=300> ;
	foaf:name	"Dave Matthews Band"@en ;
