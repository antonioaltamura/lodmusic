	dbp:website	<http://www.blacksabbath.com> .
dbr:Coldcut	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Matt_Black_-_Coldcut_(Vienna_2006).jpg?width=300> ;
	foaf:name	"Coldcut"@en ;
