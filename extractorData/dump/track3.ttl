	dbo:musicalArtist	dbr:Lionel_Richie ;
	dbo:recordLabel	dbr:Mercury_Records ,
		dbr:Island_Records ;
	dbo:releaseDate	"2006-12-11"^^xsd:date ;
	foaf:name	"Why"@en .
