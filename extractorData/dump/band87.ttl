dbr:Anal_Cunt	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Seth_with_AxCx_at_Relapse_Festival_1993_crop.jpg?width=300> ;
	foaf:name	"Anal Cunt"@en ;
	dbo:associateBand	<http://dbpedia.org/resource/Upsidedown_Cross_(band)> ,
		<http://dbpedia.org/resource/Post_Mortem_(band)> ,
		dbr:Pig_Destroyer ,
