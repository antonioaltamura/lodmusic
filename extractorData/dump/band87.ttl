dbr:Dying_Fetus	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Dyingfetus_tuska2008.jpg?width=300> ;
	foaf:name	"Dying Fetus"@en ;
	dbo:associateBand	<http://dbpedia.org/resource/Misery_Index_(band)> ,
