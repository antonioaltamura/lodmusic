<http://dbpedia.org/resource/10cc>	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/10CC_-_TopPop_1974_2.png?width=300> ;
	foaf:name	"10cc"@en ;
	dbo:associateBand	dbr:Hotlegs ,
