	dbp:currentMembers	dbr:Adolfo_Luxúria_Canibal ;
	dbp:website	<http://mao-morta.org> .
<http://dbpedia.org/resource/M\u00EAl\u00E9e_(band)>	rdf:type	dbo:Band ;
