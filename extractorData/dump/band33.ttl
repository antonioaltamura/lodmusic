dbr:Alkaline_Trio	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Alkaline_Trio_2011-07-20_01.jpg?width=300> ;
	foaf:name	"Alkaline Trio"@en ;
	dbo:associateBand	<http://dbpedia.org/resource/Heavens_(band)> ,
