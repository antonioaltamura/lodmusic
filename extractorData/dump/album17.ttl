		dbr:Blues ;
	dbo:recordLabel	dbr:Atlantic_Records ;
	dbo:releaseDate	"1979-12-03"^^xsd:date ;
	foaf:name	"The Rose (soundtrack)"@en .
dbr:The_Rude_Awakening	rdf:type	schema:MusicAlbum ;
