dbr:Mogwai	rdf:type	dbo:Band ;
	dbo:image	<http://en.wikipedia.org/wiki/Special:FilePath/Mogwai_live_RFH_2014.jpg?width=300> ;
	foaf:name	"Mogwai"@en ;
