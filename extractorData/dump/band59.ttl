dbr:Green_Day	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Green_Day_House_OF_Blues_2015_2.jpg?width=300> ;
	foaf:name	"Green Day"@en ;
