dbr:Box_Car_Racer	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Tom_DeLonge_2004.jpg?width=300> ;
	foaf:name	"Box Car Racer"@en ;
	dbo:associateBand	<http://dbpedia.org/resource/+44_(band)> ,
