dbr:Legendary_Shack_Shakers	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Legendary_Shack_Shakers_live_performance.jpg?width=300> ;
	foaf:name	"Legendary Shack Shakers"@en ;
