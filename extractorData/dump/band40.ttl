<http://dbpedia.org/resource/Jane\u0027s_Addiction>	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Janes_Addiction_@_Steel_Blue_Oval_(1_3_2010)_(4416154323).jpg?width=300> ;
