<http://dbpedia.org/resource/Information_Society_(band)>	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/INSOC_PROOFS_B_80.jpg?width=300> ;
	foaf:name	"Information Society"@en ;
