dbr:Finntroll	rdf:type	dbo:Band ;
	dbo:image	<http://commons.wikimedia.org/wiki/Special:FilePath/Finntroll_Rockharz_2016_06.jpg?width=300> ;
	foaf:name	"Finntroll"@en ;
